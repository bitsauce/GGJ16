    arrow.velocity_x = min(arrow_power, 25) * dcos(angle) * scale_x;
    arrow.velocity_y = -min(arrow_power, 25) * dsin(angle);
    arrow_power = 0.0;
}
else if (keyboard_check(key_attack)) {
    arrow_power += 10.0 / room_speed;
}
